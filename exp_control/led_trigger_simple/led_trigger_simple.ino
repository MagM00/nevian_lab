void setup() {
  pinMode(8, OUTPUT); // Set the built-in LED as output
  digitalWrite(8, HIGH); // Turn the LED on
  delay(1000); // Wait for 1800000 milliseconds (1 second)
  digitalWrite(8, LOW); // Turn the LED off
}

